<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xinzy.component">

    <application
        android:name=".ComponentApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.NoticeBoardActivity" />
        <activity android:name=".activity.PagerGridViewActivity" />
        <activity android:name=".activity.ActionSheetActivity" />
        <activity android:name=".activity.BannerViewActivity" />
        <activity android:name=".activity.DialogActivity" />
        <activity android:name=".activity.TabActivity" />
        <activity android:name=".activity.MultiAdapterActivity" />
        <activity android:name=".activity.RadiusTextViewActivity" />
        <activity android:name=".activity.TipDialogActivity" />
        <activity android:name=".activity.RadiusImageViewActivity"></activity>
    </application>

</manifest>